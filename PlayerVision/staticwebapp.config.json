{
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/manifest.json",
      "/icon.svg",
      "/assets/*",
      "/*.{js,css,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,ico,json}"
    ]
  }
}
